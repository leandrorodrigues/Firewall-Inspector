<%- model_class = Suspect -%>
<div class="page-header">
  <h1>Botlist <small>Suspect</small></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ip) %>:</strong></dt>
  <dd><%= @suspect.ip %></dd>
  <dt><strong><%= model_class.human_attribute_name(:created) %>:</strong></dt>
  <dd><%= @suspect.created %></dd>
  <dt><strong><%= model_class.human_attribute_name(:test_log) %>:</strong></dt>
  <dd><%=  simple_format(@suspect.test_log) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hits) %>:</strong></dt>
  <dd><%= @suspect.hits %></dd>
</dl>

<%= link_to t('back'), suspects_path, :class => 'btn btn-default'  %>
<%= destroy_button(@suspect) %>